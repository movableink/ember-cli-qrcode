<svg width={{size}} height={{size}} viewBox={{viewBox}} xmlns:xlink="http://www.w3.org/1999/xlink" ...attributes>
  <rect fill={{lightColor}} width="100%" height="100%"></rect>
  <rect fill={{darkColor}} width="1" height="1" id="template-{{elementId}}"></rect>

  {{#each rows as |cols y|}}
    {{#each cols as |visible x|}}
      {{#if visible}}
        <use x={{x}} y={{y}} xlink:href="#template-{{elementId}}"></use>
      {{/if}}
    {{/each}}
  {{/each}}
</svg>
